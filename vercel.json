        }
      ]
    },
    {
      "source": "/fonts/IBMPlexSans-Regular.ttf",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/IBMPlexMono-Regular.ttf",
      "headers": [
        {
